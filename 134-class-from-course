public Id classFromCourse(String courseName) {
    // code here
apxio__Class__c classnew = new apxio__Class__c();

classnew.Name = courseName;



List<apxio__Course__c> courseClassid = [SELECT id from apxio__Course__c WHERE Name =:courseName LIMIT 1];
if(courseClassid.size()>0){
classnew.apxio__Course__c = courseClassid.Id;
}


Insert classnew;
return classnew.Id;

}