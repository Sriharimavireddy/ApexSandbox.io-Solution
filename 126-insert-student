public Id insertStudent(String name, String email) {
    // Create a new student record
    apxio__Student__c studnetnew = new apxio__Student__c();
    studnetnew.Name = name;
    studnetnew.apxio__Email__c = email;

    // Insert the record
    insert studnetnew;
    
    // Return the Id directly from the record
    return studnetnew.Id;
}