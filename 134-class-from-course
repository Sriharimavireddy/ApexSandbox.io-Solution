public Id classFromCourse(String courseName) {
    // code here
apxio__Class__c classnew = new apxio__Class__c();

classnew.Name = courseName;
apxio__Course__c courseClassid = [SELECT id from apxio__Course__c WHERE Name =:courseName ];
apxio__Class__c.apxio__Course__c = courseClassid;


Insert classnew;

}